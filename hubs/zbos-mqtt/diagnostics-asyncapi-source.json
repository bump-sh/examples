                    "translationKey" : {
                      "type" : "string",
                      "description" : "status code"
                    },
                    "message" : {
                      "type" : "string",
                      "description" : "status message"
                    }
                  }
                },
                "temperature" : {
                  "type" : "string",
                  "description" : "optional, depending on robot"
                }
              }
            }
          },
          "name" : "Array<DiagnosticState>",
          "examples" : [ {
            "payload" : { }
          } ]
        }
      },
      "parameters" : {
        "key" : {
          "description" : "Request key to create a unique subscription topic",
          "schema" : {
            "type" : "string"
          }
        }
      }
